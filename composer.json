{
    "name": "flekking/sauce",
    "description": "Laravel Utilities and Support Package, hence the name.",
    "version": "0.2.0",
    "license": "Apache-2.0",
    "support": {
        "source": "https://github.com/flekking/sauce"
    },
    "authors": [
        {
            "name": "Michael Lam",
            "email": "agustinus.michael29@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "laravel/framework": ">=8.71.0",
        "barryvdh/laravel-dompdf": "^0.9.0"
